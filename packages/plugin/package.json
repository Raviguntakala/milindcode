{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@buildwise-ai/milindcode-plugin",
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./tool": {
      "development": "./src/tool.ts",
      "import": "./dist/tool.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "zod": "catalog:",
    "@buildwise-ai/milindcode-sdk": "workspace:*"
  },
  "devDependencies": {
    "@tsconfig/node22": "catalog:",
    "typescript": "catalog:"
  },
  "description": "Plugin framework for BuildWise AI Milindcode",
  "repository": {
    "type": "git",
    "url": "https://github.com/buildwise-ai/milindcode"
  }
}